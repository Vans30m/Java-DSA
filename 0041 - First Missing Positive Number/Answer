class Solution {
    public int firstMissingPositive(int[] nums) {
        Arrays.sort(nums);
        int target = 1;
        for(int i=0;i<nums.length;i++){
            if(nums[i] == target && nums[i] > 0){
                target++;
            }
            else if (nums[i] > target){
                return target;
            }
        }
        return target;
    }
}
